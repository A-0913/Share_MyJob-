<table  class="table table-sm  table-hover  p-3">
  <thead>
    <th class=table-primary>ジャンル検索</th>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
      <tr class="table-light">
        <td>
          <%= link_to jobs_path(genre_id: genre.id ) do %>
            <%= genre.name %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>